header {
  background-color: #F29441;
  height: 70px;
  color: #fff;
  font-family: "Noto Sans TC";
  font-size: 20px;
  letter-spacing: 0.1em;
}

.header_nav {
  width: 1200px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}

.header_nav_logo {
  display: inline-block;
  padding-top: 13px;
}

.header_nav_right {
  display: flex;
}

.header_nav_right > li {
  margin-left: 40px;
  position: relative;
}

.header_nav_right_inputBox {
  position: relative;
}

.header_nav_right_inputBox::before {
  content: "\f002";
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
  color: #F29441;
  position: absolute;
  left: 10px;
  top: 50%;
  transform: translate(0, -50%);
}

.header_nav_right_inputBox-input {
  display: block;
  box-sizing: border-box;
  border: 0;
  height: 34px;
  border-radius: 10px;
  width: 240px;
  margin-top: 16px;
  font-size: 18px;
  padding-left: 38px;
  color: #513F2E;
}

.header_nav_right_inputBox-input:focus {
  outline: none;
}

.header_nav_right > li > a {
  color: #fff;
  line-height: 70px;
  transition: 0.2s;
}

.header_nav_right > li:hover > a {
  color: #513F2E;
}

.header_nav_right .fa-regular {
  font-family: "Font Awesome 5 Free";
  font-weight: 900;
}

.header_nav_right > li > ol {
  overflow: hidden;
  height: 0;
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  background-color: #F29441;
  width: 120px;
  text-align: center;
  transition: 0.3s;
}

.header_nav_right > li > ol a {
  font-size: 16px;
  color: #fff;
  line-height: 50px;
  transition: 0.2s;
}

.header_nav_right > li:nth-child(2):hover > ol {
  height: 350px;
}

.header_nav_right > li:nth-child(3):hover > ol {
  height: 200px;
}

.header_nav_right > li > ol a:hover {
  background-color: #F2F2F2;
  color: #513F2E;
}

footer {
  background-color: #F29441;
  height: 250px;
  color: #fff;
  font-family: "Noto Sans TC";
  font-size: 20px;
  letter-spacing: 0.1em;
}

.footer_nav {
  width: 1200px;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 36px 0 28px 0;
}

.footer_nav_left {
  margin-right: 100px;
}

.footer_nav_left_logo {
  margin-bottom: 30px;
  width: 240px;
}

.footer_nav_left_icons {
  display: flex;
  justify-content: space-between;
  font-size: 40px;
}

.footer_nav_left_icons a {
  color: #fff;
  margin: 2px 8px;
  transition: 0.2s;
}

.footer_nav_right {
  display: flex;
  justify-content: flex-end;
  flex-grow: 1;
}

.footer_nav_right > li:nth-child(2) {
  margin-right: 100px;
}

.footer_nav_right > li > a {
  margin-bottom: 20px;
  color: #fff;
  transition: 0.2s;
}

.footer_nav_right > li:first-child > ol {
  width: 300px;
  display: flex;
  flex-wrap: wrap;
}

.footer_nav_right > li:first-child > ol > li {
  margin-right: 30px;
  line-height: 36px;
}

.footer_nav_right > li > ol > li a, .footer_nav_right > li > span {
  font-size: 16px;
  color: #fff;
  line-height: 36px;
  transition: 0.2s;
}

.footer_nav_right > li > ol > li a, .footer_nav_right > li > span > i {
  margin-right: 12px;
}

.footer_nav_left a:hover, .footer_nav_right a:hover {
  color: #513F2E;
}

.ms_header {
  background-color: #F29441;
